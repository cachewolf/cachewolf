<?xml version="1.0" encoding="ISO-8859-1"?>
<SPIDER>
	<!-- Definition der Zielvariable in CW -->
	<!-- occurance definiert, ob eine Mehrfachsuche stattfinden soll, -->
	<!-- Beispiel sind die cacheattributes -->
	<!-- recursive definiert, ob eine suche innerhalb des suchmuster erfolgen soll -->
	<!-- Beispiel sind Spoilerbilder -->
	<DBFIELD fieldname = "" occurance="single|many" recursive = "true|false">
		<!-- Definition Regex-Muster -->
		<PATTERN><![CDATA[]]></PATTERN>
		<!-- ist recursive=true muss hier nochmal so ein Eintrag erfolgen -->
		<DBFIELD>
		<DBFIELD>
		<!-- Ende: bei recursive = true -->
	</DBFIELD>
</SPIDER>