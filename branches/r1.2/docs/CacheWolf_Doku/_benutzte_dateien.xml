        <para/>
      </listitem>
      <listitem>
        <para><emphasis role="bold">&lt;spider forcelogin=&quot;true&quot; spiderUpdates=&quot;2&quot; checkLog=&quot;false&quot; maxSpiderNumer=&quot;200&quot; downloadPics=&quot;true&quot; downloadTBs=&quot;true&quot;/&gt;</emphasis></para>
        <para/>
      </listitem>
      <listitem>
        <para><emphasis role="bold">&lt;gotopanel northcentered=&quot;true&quot; /&gt;</emphasis></para>
        <para/>
      </listitem>
      <listitem>
        <para><emphasis role="bold">&lt;details cacheSize=&quot;50&quot; delete=&quot;5&quot;/&gt;</emphasis></para>
        <para/>
      </listitem>
      <listitem>
        <para><emphasis role="bold">&lt;metric type=&quot;1&quot;/&gt;</emphasis></para>
        <para/>
      </listitem>
      <listitem>
        <para><emphasis role="bold">&lt;export numberOfLogsToExport=&quot;5&quot; exportTravelBugs=&quot;false&quot; exportGpxAsMyFinds=&quot;true&quot;/&gt;</emphasis></para>
        <para/>
      </listitem>
      <listitem>
        <para><emphasis role="bold">&lt;datamover processorMode=&quot;0&quot; /&gt;</emphasis></para>
        <para/>
      </listitem>
      <listitem>
        <para><emphasis role="bold">&lt;logkeeping maximum=&quot;250&quot; keepown=&quot;true&quot; /&gt;</emphasis></para>
        <para/>
      </listitem>
      <listitem>
        <para><emphasis role="bold">&lt;fillWhiteArea on=&quot;false&quot;/&gt;</emphasis></para>
        <para/>
      </listitem>
      <listitem>
        <para><emphasis role="bold">&lt;mapLoader tileSize=&quot;1&quot; overlapping=&quot;100&quot;/&gt;</emphasis></para>
        <para/>
      </listitem>
      <listitem>
        <para><emphasis role="bold">&lt;showCachesOnMap on=&quot;true&quot;/&gt;</emphasis></para>
        <para/>
      </listitem>
      <listitem>
        <para><emphasis role="bold">&lt;SortingGroupedByCache on=&quot;true&quot;/&gt;</emphasis></para>
        <para/>
      </listitem>
      <listitem>
        <para><emphasis role="bold">&lt;Symbols useOwnSymbols=&quot;true&quot;/&gt;</emphasis></para>
        <para/>
      </listitem>
    </itemizedlist>
    <para/>
